{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "types": ["node", "express"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true
  }
}
